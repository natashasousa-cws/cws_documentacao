            "expression": [
              "",
              "VAR MaxDataVisivel =",
              "    MAX ( dCalendario[Data] ) // Pega a ltima data real do contexto (ex: 31 de janeiro)",
              "VAR AnoAtual =",
              "    YEAR ( MaxDataVisivel ) // Pega o ano dessa data",
              "RETURN",
              "    CALCULATE (",
              "        DISTINCTCOUNT ( dClientes[client_id] ),",
              "        FILTER (",
              "            ALL ( dCalendario ),",
              "            // Filtra a tabela calendrio para o perodo acumulado",
              "            YEAR ( dCalendario[Data] ) = AnoAtual",
              "                && dCalendario[Data] <= MaxDataVisivel",
              "        ),",
              "        // Ativa a relao pela chave numrica para este clculo",
              "        USERELATIONSHIP ( dCalendario[datekey], dClientes[datekey] )",
              "    )"
            ],
            "formatString": "#,0",
