              "    VAR TmT = fDatalayer_product_pageviews[max_datecreated_time]",
              "    VAR Y = INT ( Dk / 10000 )",
              "    VAR M = INT ( MOD ( INT ( Dk / 100 ), 100 ) )",
              "    VAR D = MOD ( Dk, 100 )",
              "    VAR Dt = DATE ( Y, M, D )",
              "    VAR Tm = IF ( ISBLANK ( TmT ), BLANK (), TIMEVALUE ( TmT ) )",
