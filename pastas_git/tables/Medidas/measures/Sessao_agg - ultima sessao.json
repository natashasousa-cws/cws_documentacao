{
  "name": "Sessao_agg - ultima sessao",
  "expression": [
    "",
    "VAR AnoMesDia = MAX(fDatalayer_session_summary[datekey]) -- exemplo: 20250703",
    "VAR Ano = INT(AnoMesDia / 10000)",
    "VAR Mes = INT(MOD(AnoMesDia, 10000) / 100)",
    "VAR Dia = MOD(AnoMesDia, 100)",
    "RETURN",
    "    DATE(Ano, Mes, Dia)"
  ],
  "formatString": "dd/mm/yyyy",
  "displayFolder": "Sessao_agg",
