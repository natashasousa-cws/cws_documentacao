  "expression": [
    "",
    "VAR DataMaxima = TODAY()",
    "VAR DataMinima = DataMaxima - 30",
    "VAR CategoriasVistas =",
    "    CALCULATETABLE(",
    "        DISTINCT(dProdutos[category_level_1]),",
    "        FILTER(",
    "            fDatalayer_agg_pageviews,",
    "            RELATED('dCalendario'[Data]) >= DataMinima &&",
    "            RELATED('dCalendario'[Data]) <= DataMaxima &&",
    "            fDatalayer_agg_pageviews[user_type] = \"Cliente navegando\"",
    "        )",
    "    )",
    "RETURN",
    "CONCATENATEX(",
    "    CategoriasVistas,",
    "    dProdutos[category_level_1],",
    "    \", \"",
    ")"
