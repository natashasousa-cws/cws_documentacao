{
  "name": "Data/Hora da ultima visualização de página (Cliente Navegando)",
  "expression": [
    "",
    "VAR MaxDateKey =",
    "    CALCULATE (",
    "        MAX ( fDatalayer_agg_pageviews[datekey_max_datecreated] ),",
    "        fDatalayer_agg_pageviews[user_type] = \"Cliente Navegando\"",
    "    )",
    "VAR BaseDia =",
    "    DATE (",
    "        INT ( MaxDateKey / 10000 ),",
    "        INT ( MOD ( INT ( MaxDateKey / 100 ), 100 ) ),",
    "        INT ( MOD ( MaxDateKey, 100 ) )",
    "    )",
    "VAR BaseDiaRows =",
    "    FILTER (",
    "        CALCULATETABLE (",
    "            fDatalayer_agg_pageviews,",
    "            fDatalayer_agg_pageviews[user_type] = \"Cliente Navegando\"",
    "        ),",
    "        fDatalayer_agg_pageviews[datekey_max_datecreated] = MaxDateKey",
    "    )",
    "VAR MaxHoraTxt =",
    "    MAXX ( BaseDiaRows, fDatalayer_agg_pageviews[max_datecreated_time] )",
    "VAR HoraNum =",
    "    TIMEVALUE ( MaxHoraTxt )",
    "RETURN",
    "IF (",
    "    ISBLANK ( MaxDateKey ),",
    "    BLANK(),",
    "    BaseDia + HoraNum",
    ")"
  ],
  "formatString": "General Date",
  "lineageTag": "67f26ada-3f13-4db7-b44e-16e2ba8d99af"
}