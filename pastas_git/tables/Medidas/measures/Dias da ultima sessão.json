  "expression": [
    "",
    "VAR DataUltimaSessao =",
    "    CALCULATE(",
    "        MAX(dCalendario[data]),",
    "        FILTER(",
    "            fDatalayer_session_summary,",
    "            fDatalayer_session_summary[user_type] = \"Cliente navegando\"",
    "        )",
    "    )",
    "RETURN",
    "IF(",
    "    NOT ISBLANK(DataUltimaSessao),",
    "    DATEDIFF(DataUltimaSessao, TODAY(), DAY),",
    "    BLANK()",
    ")"
  ],
