{
  "name": "Frequencia de pedidos",
  "expression": [
    "",
    "VAR dtmin =",
    "    CALCULATE (",
    "        CONVERT ( MIN ( fVendas[datekey] ), DATETIME )",
    "    )",
    "VAR ciclo =",
    "    DATEDIFF ( dtmin, TODAY (), DAY )",
    "RETURN",
    "    DIVIDE ( ciclo, [Pedidos] )"
  ],
  "formatString": "#,0.0",
